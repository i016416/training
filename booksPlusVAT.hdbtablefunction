FUNCTION "STUDENT01"."STUDENT01::booksPlusVAT" (vat decimal(5,3) ) 
	RETURNS "STUDENT01::demo.booksPlusVAT"
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER AS
BEGIN
return 
SELECT  "PUBLISHER", "ISBN", "TITLE", "YEAR", "EDITION", "PRICE", price* vat as "PRICEPLUSVAT", "CURRENCY", "NAME", "STREET", "POST_CODE", "CITY", "COUNTRY"
FROM "STUDENT01"."STUDENT01::demo.BookDetails" ;
END;
